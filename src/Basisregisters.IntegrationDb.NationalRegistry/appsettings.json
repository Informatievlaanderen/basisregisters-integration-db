{
  "ConnectionStrings": {
    "Integration": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.BuildingRegistry;Trusted_Connection=True;TrustServerCertificate=True;",
    "IntegrationAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.BuildingRegistry;Trusted_Connection=True;TrustServerCertificate=True;"
  },

  "NationalRegistry": {
    "Url": "",
    "Username": "",
    "Password": ""
  },

  "Magda": {
    "Url": "",
    "Username": "",
    "Password": ""
  },

  "TopicArn": "x",

  "DistributedLock": {
    "Region": "eu-west-1",
    "TableName": "__DistributedLocks__",
    "LeasePeriodInMinutes": 5,
    "ThrowOnFailedRenew": true,
    "TerminateApplicationOnFailedRenew": true
  },

  "DataDog": {
    "Enabled": true,
    "Debug": false,
    "ServiceName": "national-registry"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "NationalRegistry",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  }
}
